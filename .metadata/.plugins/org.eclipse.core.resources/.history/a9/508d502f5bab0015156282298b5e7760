package QueueStack;

public class PolishCalc {
	static Stack<Integer> stack = new Stack();
	
	public static void main(String[] args) {
		String test = "94+";
		calcPol(test);
	}
	public static int calcPol(String n) {
		int conv = n.charAt(0);
		if (conv > 47 && conv < 58) {
			stack.push(conv);
			return calcPol(n.substring(1, n.length()));
		}
		switch(n.charAt(0)) {
		int temp = stack.pop();
		case '+': 
			return stack.pop() + temp;
		case '-':
			return stack.pop() - temp;
		case '*':
			return stack.pop() * temp;
		case 'x':
			return stack.pop() / temp;
		case '/':
			return stack.pop() / temp;
		case '%':
			return stack.pop() % temp;
		default:
			System.out.println("Invalid input.");
			System.exit(1);
		}
	}
}
